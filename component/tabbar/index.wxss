/* custom-tab-bar/index.wxss */

.tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  height: calc(100rpx + env(safe-area-inset-bottom));
  display: flex;
  justify-content: space-around;
  color: black;
  z-index: 9999;
  font-size: 20rpx;
  padding: 20rpx 0;
}

.tabbar-box {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.the-land-img,
.explore-img,
.account-img {
  color: black;
  width: 60rpx;
  height: 60rpx;
  display: inline-block;
  vertical-align: middle;
  /* margin-top: 10rpx; */
  margin-bottom: 10rpx;
}

.the-land-text,
.explore-text,
.account-text {
  white-space: nowrap;

}